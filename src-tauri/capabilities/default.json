{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:window:default",
    "core:window:allow-start-dragging",
    "store:default",
    {
      "identifier":"opener:allow-open-url",
      "allow": [
        {
          "url": "https://github.com/wrtx-dev/deeproxy"
        }
      ]
    }
  ]
}